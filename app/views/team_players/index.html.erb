<a href="/players">Players Index</a>

<h1><%= @team.name %></h1>

<% @players.each do |player| %>
   <h3>Name: <%= link_to player.name, "/players/#{player.id}" %></h3>
   <p>Position: <%= player.position %></p>
   <p>Jersey Number: <%= player.jersey_number %></p>
   <p>Injuried: <%= player.injuried %></p><br/>
<% end %>

